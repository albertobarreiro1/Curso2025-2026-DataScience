################################################################################
# DISTRICTS (para puntos de recarga)
################################################################################

<#TriplesMap_District>
  a rr:TriplesMap ;
  rml:logicalSource [
    rml:source "../data/PUNTOS-PUBLICOS-RECARGA-VEHICULOS-ELECTRICOS-updated.csv" ;
    rml:referenceFormulation ql:CSV
  ] ;
  rr:subjectMap [
    rr:template "https://group10.linkeddata.es/resource/district/{districtCode}" ;
    rr:class ont:District
  ] ;
  rr:predicateObjectMap [
    rr:predicate ont:districtCode ;
    rr:objectMap [ rml:reference "districtCode" ; rr:datatype xsd:integer ]
  ] ;
  rr:predicateObjectMap [
    rr:predicate rdfs:label ;
    rr:objectMap [ rml:reference "district" ]
  ] .

################################################################################
# CHARGING POINTS
################################################################################

<#TriplesMap_ChargingPoint>
  a rr:TriplesMap ;
  rml:logicalSource [
    rml:source "../data/PUNTOS-PUBLICOS-RECARGA-VEHICULOS-ELECTRICOS-updated.csv" ;
    rml:referenceFormulation ql:CSV
  ] ;
  rr:subjectMap [
    rr:template "https://group10.linkeddata.es/resource/charging-point/{id}" ;
    rr:class ont:ChargingPoint
  ] ;
  rr:predicateObjectMap [
    rr:predicate ont:hasPointId ;
    rr:objectMap [ rml:reference "id" ; rr:datatype xsd:string ]
  ] ;
  rr:predicateObjectMap [
    rr:predicate ont:address ;
    rr:objectMap [ rml:reference "address" ; rr:datatype xsd:string ]
  ] ;
  rr:predicateObjectMap [
    rr:predicate ont:site ;
    rr:objectMap [ rml:reference "site" ; rr:datatype xsd:string ]
  ] ;
  rr:predicateObjectMap [
    rr:predicate ont:additionalInfo ;
    rr:objectMap [ rml:reference "additionalInfo" ; rr:datatype xsd:string ]
  ] ;
  rr:predicateObjectMap [
    rr:predicate ont:equipmentNumber ;
    rr:objectMap [ rml:reference "equipmentNumber" ; rr:datatype xsd:integer ]
  ] ;
  rr:predicateObjectMap [
    rr:predicate ont:features ;
    rr:objectMap [ rml:reference "features" ; rr:datatype xsd:string ]
  ] ;
  rr:predicateObjectMap [
    rr:predicate ont:operator ;
    rr:objectMap [ rml:reference "operator" ; rr:datatype xsd:string ]
  ] ;
  rr:predicateObjectMap [
    rr:predicate ont:openingHours ;
    rr:objectMap [ rml:reference "openingHours" ; rr:datatype xsd:string ]
  ] ;
  rr:predicateObjectMap [
    rr:predicate ont:management ;
    rr:objectMap [ rml:reference "management" ; rr:datatype xsd:string ]
  ] ;
  rr:predicateObjectMap [
    rr:predicate ont:inDistrict ;
    rr:objectMap [ rr:template "https://group10.linkeddata.es/resource/district/{districtCode}" ]
  ] ;
  rr:predicateObjectMap [
    rr:predicate wgs84_pos:lat ;
    rr:objectMap [ rml:reference "utmY" ; rr:datatype xsd:decimal ]
  ] ;
  rr:predicateObjectMap [
    rr:predicate wgs84_pos:long ;
    rr:objectMap [ rml:reference "utmX" ; rr:datatype xsd:decimal ]
  ] .
