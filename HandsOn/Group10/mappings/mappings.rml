@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://www.w3.org/ns/ql#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix ont: <https://group10.linkeddata.es/ontology/> .
@prefix wgs84_pos: <http://www.w3.org/2003/01/geo/wgs84_pos#> .

################################################################################
# PERMANENT STATIONS (trafico)
################################################################################

<#TriplesMap_PermanentStation>
  a rr:TriplesMap ;
  rml:logicalSource [
    rml:source "../data/DATOS-ESTACIONES-MARZO-2025-updated.csv" ;
    rml:referenceFormulation ql:CSV
  ] ;
  rr:subjectMap [
    rr:template "https://group10.linkeddata.es/resource/station/{stationId}" ;
    rr:class ont:PermanentStation
  ] ;
  rr:predicateObjectMap [
    rr:predicate ont:hasStationId ;
    rr:objectMap [ rml:reference "stationId" ; rr:datatype xsd:string ]
  ] .

################################################################################
# TRAFFIC OBSERVATIONS
################################################################################

<#TriplesMap_TrafficObservation>
  a rr:TriplesMap ;
  rml:logicalSource [
    rml:source "../data/DATOS-ESTACIONES-MARZO-2025-updated.csv" ;
    rml:referenceFormulation ql:CSV
  ] ;
  rr:subjectMap [
    rr:template "https://group10.linkeddata.es/resource/observation/{id}" ;
